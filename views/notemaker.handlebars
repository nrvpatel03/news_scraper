<div class="container">
<p>Hello</p>
<p id="key">{{article._id}}</p>

<form>
<div class="form-group">
    <label for="title">Title</label>
    <textarea class="form-control" id="title" rows="1">{{article.note.title}}</textarea>
</div>
<div class="form-group">
    <label for="body">Body</label>
    <textarea class="form-control" id="body" rows="3">{{article.note.body}}</textarea>
</div>
<button type="submit" id="submitNote">Submit/Update Note</button>
</form>
<a href="/"><button class="btn-lg btn-danger">Home</button></a>
</div>
<script src="/assets/js/notemaker.js"></script>