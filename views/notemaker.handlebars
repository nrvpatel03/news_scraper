<div class="scraped container">
    <p id="key" style="display: none">{{article._id}}</p>
<form>
<div class="form-group">
    <label class="formLabels" for="title">Title</label>
    <textarea class="form-control" id="title" rows="1">{{article.note.title}}</textarea>
</div>
<div class="form-group">
    <label class="formLabels" for="body">Body</label>
    <textarea class="form-control" id="body" rows="3">{{article.note.body}}</textarea>
</div>
<button class="homebtn btn-lg btn-block btn-warning" type="submit" id="submitNote">Submit/Update Note</button>
</form>
<a href="/"><button class="btn-lg btn-danger btn-block">Home</button></a>
</div>
<script src="/assets/js/notemaker.js"></script>